{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=5.6.4",
    "laravel/framework": "5.3.*",
    "caffeinated/modules": "^4.0",
    "stolz/assets": "^0.1.8",
    "bosnadev/repositories": " 0.*",
    "laravelcollective/html": "^5.2.0",
    "davejamesmiller/laravel-breadcrumbs": "^3.0",
    "tormjens/eventy": "dev-master",
    "laravel/cashier": "~7.0",
    "caffeinated/shinobi": "^3.0",
    "orangehill/iseed": "dev-master",
    "maatwebsite/excel": "~2.1.0",
    "intervention/image": "dev-master",
    "chumper/zipper": "0.6.x",
    "yajra/laravel-datatables-oracle": "~6.0",
    "doctrine/dbal": "^2.5",
    "sahak.avatar/avatar": "dev-master"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~5.0",
    "symfony/css-selector": "3.1.*",
    "symfony/dom-crawler": "3.1.*"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers/API/api.php",
      "app/helpers/shortcode.php",
      "app/Core/standards/constants.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan optimize"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
